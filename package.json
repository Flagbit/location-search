{
  "name": "@flagbit/location-search",
  "version": "0.1.0",
  "main": "index.html",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json --type-check --project tsconfig.json '**/*.ts' --exclude '**/node_modules/**'",
    "build": "yarn lint && tsc",
    "test:bs": "karma start karma.conf.js",
    "test": "karma start karma.local.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e/protractor.conf.js"
  },
  "dependencies": {
    "angular": "1.6.4",
    "angular-utils-pagination": "^0.11.1",
    "rxjs": "^5.4.3",
    "systemjs": "0.19.39"
  },
  "devDependencies": {
    "@types/angular": "^1.6.24",
    "@types/angular-mocks": "^1.5.10",
    "@types/googlemaps": "^3.26.20",
    "angular-mocks": "^1.6.4",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-requirejs": "^1.1.0",
    "protractor": "^5.1.2",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  }
}
