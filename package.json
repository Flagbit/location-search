{
  "name": "@flagbit/location-search",
  "version": "0.2.9",
  "main": "index.html",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json --type-check --project tsconfig.json '**/*.ts' --exclude '**/node_modules/**' --exclude '**/worker/**'",
    "build": "yarn lint && tsc && cd lib/shared/worker && tsc && cd ../../../",
    "test:bs": "karma start karma.conf.js",
    "test": "karma start karma.local.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e/protractor.conf.js",
    "webpack": "webpack",
    "compress": "yarn uglify:module && yarn uglify:worker",
    "uglify:module": "./node_modules/uglify-js/bin/uglifyjs dist/location-search.js --compress > dist/location-search.min.js",
    "uglify:worker": "./node_modules/uglify-js/bin/uglifyjs dist/data.worker.js --compress > dist/data.worker.min.js",
    "dist": "yarn lint && yarn webpack && cd lib/shared/worker && tsc && cd ../../../ && cp lib/shared/worker/data.worker.js dist/ && yarn compress",
    "prepublish": "yarn dist"
  },
  "dependencies": {
    "angular": "1.6.4",
    "angular-utils-pagination": "^0.11.1",
    "rxjs": "^5.4.3",
    "systemjs": "0.19.39",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/angular": "^1.6.24",
    "@types/angular-mocks": "^1.5.10",
    "@types/googlemaps": "^3.26.20",
    "@types/jasmine": "^2.5.54",
    "angular-mocks": "^1.6.4",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-requirejs": "^1.1.0",
    "protractor": "^5.1.2",
    "ts-loader": "^2.3.3",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2",
    "uglifyjs": "^2.4.11",
    "webpack": "^3.5.5"
  }
}
